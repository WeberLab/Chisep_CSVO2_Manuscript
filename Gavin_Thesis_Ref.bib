@article{ruettenIntroductionQuantitativeSusceptibility2019,
  title = {Introduction to {{Quantitative Susceptibility Mapping}} and {{Susceptibility Weighted Imaging}}},
  author = {Ruetten, Pascal P. R. and Gillard, Jonathan H. and Graves, Martin J.},
  year = {2019},
