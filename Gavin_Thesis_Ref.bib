@article{yadavDualImagingModalityApproach2020,
  title = {Dual-{{Imaging Modality Approach}} to {{Evaluate Cerebral Hemodynamics}} in {{Growth-Restricted Fetuses}}: {{Oxygenation}} and {{Perfusion}}},
  shorttitle = {Dual-{{Imaging Modality Approach}} to {{Evaluate Cerebral Hemodynamics}} in {{Growth-Restricted Fetuses}}},
  author = {Yadav, Brijesh~Kumar and {Hernandez-Andrade}, Edgar and Krishnamurthy, Uday and Buch, Sagar and Jella, Pavan and Trifan, Anabela and Yeo, Lami and Hassan, Sonia~S. and Haacke, Ewart~Mark and Romero, Roberto and Neelavalli, Jaladhar},
  year = {2020},
  journal = {Fetal Diagnosis and Therapy},
  volume = {47},
  number = {2},
  pages = {145--155},
  issn = {1015-3837, 1421-9964},
  doi = {10.1159/000500954},
  urldate = {2024-02-17},
  abstract = {\textbf{\emph{Objective:}} To evaluate a dual-imaging modality approach to obtain a combined estimation of venous blood oxygenation (S\emph{{$\nu$}O}\textsubscript{2}) using susceptibility-weighted magnetic resonance imaging (SWI-MRI), and blood perfusion using power Dopp\-ler ultrasound (PDU) and fractional moving blood volume (FMBV) in the brain of normal growth and growth-restricted fetuses. \textbf{\emph{Methods:}} Normal growth (\emph{n} = 33) and growth-restricted fetuses (\emph{n} = 10) from singleton pregnancies between 20 and 40 weeks of gestation were evaluated. MRI was performed and S\emph{{$\nu$}O}\textsubscript{2} was calculated using SWI-MRI data obtained in the straight section of the superior sagittal sinus. Blood perfusion was estimated using PDU and FMBV from the frontal lobe in a mid-sagittal plane of the fetal brain. The association between fetal brain S\emph{{$\nu$}O}\textsubscript{2} and FMBV, and the distribution of S\emph{{$\nu$}O}\textsubscript{2} and FMBV values across gestation were calculated for both groups. \textbf{\emph{Results:}} In growth-restricted fetuses, the brain S\emph{{$\nu$}O}\textsubscript{2} values were similar, and the FMBV values were higher across gestation as compared to normal growth fetuses. There was a significantly positive association between S\emph{{$\nu$}O}\textsubscript{2} and FMBV values (slope = 0.38 {\textpm} 0.12; \emph{r} = 0.7; \emph{p} = 0.02) in growth-restricted fetuses. In normal growth fetuses, S\emph{{$\nu$}O}\textsubscript{2} showed a mild decreasing trend (slope = --0.7 {\textpm} 0.4; \emph{p} = 0.1), whereas FMBV showed a mild increasing trend (slope = 0.2 {\textpm} 0.2; \emph{p} = 0.2) with advancing gestation, and a mild but significant negative association (slope = --0.78 {\textpm} 0.3; \emph{r} = --0.4; \emph{p} = 0.04) between these two estimates. \textbf{\emph{Conclusion:}} Combined MRI (SWI) and ultrasound (FMBV) techniques showed a significant association between cerebral blood oxygenation and blood perfusion in normal growth and growth-restricted fetuses. This dual-imaging approach could contribute to the early detection of fetal ``brain sparing'' and brain oxygen saturation changes in high-risk pregnancies.},
  langid = {english}
}

@article{yadavImagingPutativeFetal2018,
  title = {Imaging Putative Fetal Cerebral Blood Oxygenation Using {{Susceptibility Weighted Imaging}} ({{SWI}})},
  author = {Yadav, Brijesh Kumar and Krishnamurthy, Uday and Buch, Sagar and Jella, Pavan and {Hernandez-Andrade}, Edgar and Yeo, Lami and Korzeniewski, Steven J. and Trifan, Anabela and Hassan, Sonia S. and Haacke, E. Mark and Romero, Roberto and Neelavalli, Jaladhar},
  year = {2018},
  month = may,
  journal = {European radiology},
  volume = {28},
  number = {5},
  pages = {1884--1890},
  issn = {0938-7994},
  doi = {10.1007/s00330-017-5160-x},
  urldate = {2024-03-18},
  abstract = {Objective To evaluate the magnetic susceptibility, {$\Delta\chi$}v as a surrogate marker of venous blood oxygen saturation, SvO2, in second- and third-trimester normal human fetuses. Methods Thirty-six pregnant women, having a mean gestational age (GA) of 31 2/7 weeks, underwent magnetic resonance imaging (MRI). Susceptibility weighted imaging (SWI) data from the fetal brain was acquired. {$\Delta\chi$}v, of the superior sagittal sinus (SSS) was quantified using MR susceptometry from the intra-vascular phase measurements. Assuming the magnetic property of fetal blood, {$\Delta\chi$}do, is the same as that of adult blood, SvO2 was derived from the measured {$\Delta\chi$}v. The variation of {$\Delta\chi$}v and SvO2, as a function of GA, was statistically evaluated. Results The mean {$\Delta\chi$}v in the SSS in the second-trimester (n=8) and third-trimester fetuses (n=28) was found to be 0.34{\textpm}0.06ppm and 0.49{\textpm}0.05ppm, respectively. Correspondingly, the derived SvO2 values were 69.4\%{\textpm}3.27\% and 62.6\%{\textpm}3.25\%. Although not statistically significant, an increasing trend (p=0.08) in {$\Delta\chi$}v and a decreasing trend (p=0.22) in SvO2, with respect to advancing gestation was observed. Conclusion We report cerebral venous blood magnetic susceptibility and putative oxygen saturation in healthy human fetuses. Cerebral oxygen saturation in healthy human fetuses, despite a slight decreasing trend, does not change significantly with advancing gestation.},
  pmcid = {PMC5884705},
  pmid = {29247352}
}

@article{yangEvaluationWholebrainOxygen2023,
  title = {Evaluation of Whole-Brain Oxygen Metabolism in {{Alzheimer}}'s Disease Using {{QSM}} and Quantitative {{BOLD}}},
  author = {Yang, Aocai and Zhuang, Hangwei and Du, Lei and Liu, Bing and Lv, Kuan and Luan, Jixin and Hu, Pianpian and Chen, Feng and Wu, Kai and Shu, Ni and Shmuel, Amir and Ma, Guolin and Wang, Yi},
  year = {2023},
  month = nov,
  journal = {NeuroImage},
  volume = {282},
  pages = {120381},
  issn = {1095-9572},
  doi = {10.1016/j.neuroimage.2023.120381},
  abstract = {OBJECTIVE: The objective of this study was to evaluate the whole-brain pattern of oxygen extraction fraction (OEF), cerebral blood flow (CBF), and cerebral metabolic rate of oxygen consumption (CMRO2) perturbation in Alzheimer's disease (AD) and investigate the relationship between regional cerebral oxygen metabolism and global cognition. METHODS: Twenty-six AD patients and 25 age-matched healthy controls (HC) were prospectively recruited in this study. Mini-Mental State Examination (MMSE) was used to evaluate cognitive status. We applied the QQ-CCTV algorithm which combines quantitative susceptibility mapping and quantitative blood oxygen level-dependent models (QQ) for OEF calculation. CBF map was computed from arterial spin labeling and CMRO2 was generated based on Fick's principle. Whole-brain and regional OEF, CBF, and CMRO2 analyses were performed. The associations between these measures in substructures of deep brain gray matter and MMSE scores were assessed. RESULTS: Whole brain voxel-wise analysis showed that CBF and CMRO2 values significantly decreased in AD predominantly in the bilateral angular gyrus, precuneus gyrus and parieto-temporal regions. Regional analysis showed that CBF value decreased in the bilateral caudal hippocampus and left rostral hippocampus and CMRO2 value decreased in left caudal and rostral hippocampus in AD patients. Considering all subjects in the AD and HC groups combined, the mean CBF and CMRO2 values in the bilateral hippocampus positively correlated with the MMSE score. CONCLUSION: CMRO2 mapping with the QQ-CCTV method - which is readily available in MR systems for clinical practice - can be a potential biomarker for AD. In addition, CMRO2 in the hippocampus may be a useful tool for monitoring cognitive impairment.},
  langid = {english},
  pmid = {37734476},
  keywords = {Alzheimer Disease,Alzheimer's disease,Brain,Cerebral blood flow,Cerebral metabolic rate of oxygen consumption,Cerebrovascular Circulation,Gray Matter,Humans,Magnetic Resonance Imaging,Oxygen,Oxygen Consumption,Oxygen extraction fraction,Quantitative susceptibility mapping,Respiratory Function Tests}
}

